{
  "name": "tvl-bucket",
  "version": "1.0.0",
  "description": "Video upload api for lifting-video-log",
  "main": "bin/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc",
    "serve": "node dist/bin/server.js",
    "dev": "nodemon src/bin/server.ts",
    "generate:api": "node bin/generate-api-key.js",
    "migrate:make": "knex migrate:make --knexfile=./database/knexfile.js",
    "migrate:latest": "knex migrate:latest --knexfile=./database/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile=./database/knexfile.js",
    "seed:make": "knex seed:make --knexfile=./database/knexfile.js",
    "seed:run": "knex seed:run --knexfile=./database/knexfile.js"
  },
  "keywords": [
    "video",
    "logging",
    "lifting",
    "powerlifting",
    "fitness"
  ],
  "author": "wajeht",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "knex": "^1.0.1",
    "multer": "^1.4.4",
    "pg": "^8.7.1",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.16",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
